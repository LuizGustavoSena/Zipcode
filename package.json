{
  "name": "zipcode",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node --env-file=.env.dev --import tsx src/server.ts",
    "build": "tsc",
    "knex:make:dev": "node --env-file=.env.dev ./node_modules/knex/bin/cli.js migrate:make",
    "knex:latest:dev": "node --env-file=.env.dev ./node_modules/knex/bin/cli.js migrate:latest",
    "knex:latest:prd": "node --env-file=.env.prd ./node_modules/knex/bin/cli.js migrate:latest",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "8.5.0",
    "axios": "1.4.0",
    "dotenv": "16.3.1",
    "fastify": "4.24.3",
    "knex": "3.1.0",
    "moment": "2.30.1",
    "pg": "8.18.0",
    "uuid": "13.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@types/axios": "0.14.0",
    "@types/jwt-simple": "0.5.36",
    "@types/node": "20.8.9",
    "cross-env": "10.1.0",
    "dotenv-cli": "11.0.0",
    "eslint": "8.52.0",
    "ts-node": "10.9.1",
    "tsx": "3.14.0",
    "typescript": "5.9.3",
    "vitest": "0.34.6",
    "vitest-mock-extended": "1.3.1"
  },
  "repository": "https://github.com/LuizGustavoSena/Zipcode.git",
